<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wiky.customui.pageview.PageViewActivity">

    <com.wiky.ui.container.PageView
        android:id="@+id/page_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:nextBreakPoint="0.3"
        app:nextDuration="200"
        app:prevBreakPoint="0.5"
        app:resetCurrentDuration="150"
        app:resetPrevDuration="150"
        app:ri="1.5" />

</RelativeLayout>
